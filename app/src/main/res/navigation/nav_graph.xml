<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.noor.periodictable.ui.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_elementDialogFragment"
            app:destination="@id/elementDialogFragment" />
    </fragment>
    <fragment
        android:id="@+id/elementDialogFragment"
        android:name="com.noor.periodictable.ui.ElementDialogFragment"
        android:label="ElementDialogFragment" >
        <action
            android:id="@+id/action_elementDialogFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
</navigation>